#include <stdio.h>

int isPrime(int num) {
    int i;
    for(i=2; i<=num/2; i++) {
        if(num%i == 0) {
            return 0;
        }
    }
    return 1;
}

int main() {
    int start, end, i, count=0;
    
    printf("Enter the starting number: ");
    scanf("%d", &start);
    
    printf("Enter the ending number: ");
    scanf("%d", &end);
    
    for(i=start; i<=end; i++) {
        if(isPrime(i)) {
            count++;
        }
    }
    
    printf("The count of prime numbers between %d and %d is %d", start, end, count);
    
    return 0;
}
